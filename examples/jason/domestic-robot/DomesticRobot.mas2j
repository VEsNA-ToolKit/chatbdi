/* Jason Project

   See Prometheus specification in doc folder

*/

MAS domestic_robot {
    environment: HouseEnv(gui) // use "nogui" as parameter to not show the GUI
    agents: robot;
            owner;
            supermarket;

            user user.asl [
                nl2log_prompt =     "../../../interpreter/src/agt/chatbdi/modelfiles/nl2logPrompt.txt",
                log2nl_prompt =     "../../../interpreter/src/agt/chatbdi/modelfiles/log2nlPrompt.txt",
                nl2log_model =      "../../../interpreter/src/agt/chatbdi/modelfiles/nl2log.txt",
                log2nl_model =      "../../../interpreter/src/agt/chatbdi/modelfiles/log2nl.txt",
                class_model =       "../../../interpreter/src/agt/chatbdi/modelfiles/classifier.txt",
                gen_model = "qwen2.5-coder",
                emb_model = "all-minilm"
            ]
            agentArchClass chatbdi.Interpreter;
}
