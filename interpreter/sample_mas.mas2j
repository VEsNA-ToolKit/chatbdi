MAS sample_mas {

    agents:
        bob     bob.asl;
        alice   alice.asl [
            nl2log_prompt = "src/agt/interpreter/modelfiles/nl2logPrompt.txt",
            log2nl_prompt = "src/agt/interpreter/modelfiles/log2nlPrompt.txt",
            nl2log_model = "src/agt/interpreter/modelfiles/nl2log.txt",
            log2nl_model = "src/agt/interpreter/modelfiles/log2nl.txt",
            class_model = "src/agt/interpreter/modelfiles/classifier.txt",
            gen_model = "qwen2.5-coder",
            emb_model = "all-minilm"
        ]
        agentArchClass chatbdi.Interpreter;
    
    aslSourcePath:
        "src/agt";
}